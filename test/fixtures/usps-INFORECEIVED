<script src="/global-elements/lib/script/requirejs/require.js"></script>
<script type="text/javascript" src="/global-elements/lib/script/jquery/dist/jquery.min.js"></script>
<script src="/global-elements/lib/script/modernizr/modernizr.js"></script>
<script type="text/javascript">
	var GlobalJQueryv1_11 = jQuery.noConflict(true);
        var locked = false;

        function lockScroll() {
            // lock scroll position, but retain settings for later

            GlobalJQueryv1_11("html").addClass("noscroll");
            GlobalJQueryv1_11("body").addClass("noscroll");

            locked = true;
        }

        function unlockScroll() {
            // un-lock scroll position

            GlobalJQueryv1_11("html").removeClass("noscroll");
            GlobalJQueryv1_11("body").removeClass("noscroll");

            locked = false;
        };

        function resetContent() {
            GlobalJQueryv1_11('#ge-overlay').removeClass('active');

            GlobalJQueryv1_11('.quicktools-title p').removeClass('full');

            GlobalJQueryv1_11(".menu--tier-one-category--search").removeClass('active');

            unlockScroll();

            //GlobalJQueryv1_11('.menu--tier-one-category--search').removeClass('active');
            //GlobalJQueryv1_11('.mobile-header .search-trigger').removeClass('active');
            //GlobalJQueryv1_11('#search--display').removeClass("active");
            //GlobalJQueryv1_11('#search--display').css("display", 'none');

            //GlobalJQueryv1_11('#shortcuts-menu').removeClass('active');
        };

        function positionSearchOverlay() {
            var mq = window.matchMedia("(min-width: 700px)");

            if (mq.matches) {
                var topOff = GlobalJQueryv1_11(
                        '.menu--tier-one-category--search a.menu--tier-one-link.menu--item').offset()
                    .top +
                    GlobalJQueryv1_11('.menu--tier-one-category--search a.menu--tier-one-link.menu--item').height() -
                    7;
                var leftOff = GlobalJQueryv1_11(
                        'li.menu--tier-one-category--search a.menu--tier-one-link.menu--item').offset()
                    .left - 518;
                GlobalJQueryv1_11('#search--display.active').css("top", topOff);
                GlobalJQueryv1_11('#search--display.active').css("left", leftOff);

                /*
                document.ontouchstart = function(e) {
                    e.preventDefault();
                }
                */
            } else {
                document.ontouchstart = function(e) {
                    return true;
                }
            }
        };

        function touchFixTabletLandscape() {
            var mq = window.matchMedia("(min-width: 700px)");

            if (mq.matches) {
                if (GlobalJQueryv1_11("html").hasClass("touch")) {
                    //alert("stuff");
                    //The script below should be wrapped in a Modernizr check for touchstart  and also something to check that we are on a tablet, otherwise the logic below will impact the mobile nav.

                    GlobalJQueryv1_11(".menu--tier-one-category--mail-ship .menu--tier-one-link, .menu--tier-one-category--help .menu--tier-one-link, .menu--tier-one-category--international .menu--tier-one-link, .menu--tier-one-category--business .menu--tier-one-link, .menu--tier-one-category--postal-store .menu--tier-one-link, .menu--tier-one-category--track-manage .menu--tier-one-link")
                        .on("touchstart click", function(event) {
                        //prevent the default link navigation
                        event.preventDefault();

                        //caching link selector for performance
                        GlobalJQueryv1_11this = GlobalJQueryv1_11(this);

                        //check if link has been triggeed once. if so user is probably trying to go to top level link so navigate there
                        if (GlobalJQueryv1_11this.data('clicked-once')) {
                            //window.location = GlobalJQueryv1_11this.attr('href');
                        } else {
                            //find the submenu and display it, then set that weve clicked this once
                            GlobalJQueryv1_11this.find(".menu--tier-two").css("opacity", 1);
                            GlobalJQueryv1_11this.data('clicked-once', true);
                            //touchFixTabletLandscape();
                        }
                    });
                } else {}
            } else {}
        };

        function closeAll() {
            GlobalJQueryv1_11('.qt-close').removeClass('active');
            GlobalJQueryv1_11('.quicktools-open').css("height", "0px");
            GlobalJQueryv1_11('.quicktools-full').css('height', '0px');
            GlobalJQueryv1_11('.quicktools-title a p').text('Quick Tools').removeClass('partial');
            GlobalJQueryv1_11('#global-menu.menu').removeClass('active');

            GlobalJQueryv1_11('.quicktools-open').removeClass('active');
            GlobalJQueryv1_11('.mobile-header .search-trigger').removeClass('active');

            GlobalJQueryv1_11('.mobile-header .mobile-menu-trigger').removeClass('active');
            GlobalJQueryv1_11('#ge-overlay').removeClass('active');
            GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');
            //unlockScroll();
        }

        touchFixTabletLandscape();

        GlobalJQueryv1_11(window).resize(function() {
            resetContent();
            positionSearchOverlay();

            var mq = window.matchMedia("(min-width: 966px)");
            if (mq.matches) {
                GlobalJQueryv1_11('#search--display').removeClass("active");
                GlobalJQueryv1_11('#search--display').css("display", 'none');

                GlobalJQueryv1_11("#shortcuts-menu").removeClass("active");
            }
        });

        GlobalJQueryv1_11(window).bind('orientationchange', function(event) {
            resetContent();
            positionSearchOverlay();
            window.location.reload();
            touchFixTabletLandscape();
        });


        //START SEARCH
        GlobalJQueryv1_11('li.menu--tier-one-category--search a.menu--tier-one-link').on('click', function(e) {
                GlobalJQueryv1_11('li.menu--tier-one-category--search').trigger('click');

                GlobalJQueryv1_11("#ge-overlay").addClass("active");

                positionSearchOverlay();
            });


        GlobalJQueryv1_11(".menu--tier-one-category--search, .menu--tier-one-category--search a, .search-close").on('click', function(e) {
            GlobalJQueryv1_11(".menu--tier-one-category--search").toggleClass("active");
            GlobalJQueryv1_11('#search--display').toggleClass("active");

            positionSearchOverlay();

            if (GlobalJQueryv1_11('#search--display').hasClass('active')) {

                GlobalJQueryv1_11('#ge-overlay').addClass('active');
                lockScroll();
            } else {
                GlobalJQueryv1_11('#ge-overlay').removeClass('active');
                unlockScroll();
                GlobalJQueryv1_11('.menu--tier-one-category--search').removeClass('active');

            };

            if (GlobalJQueryv1_11('#search--display').hasClass('active')) {
                GlobalJQueryv1_11('.q').focus();
            }

            positionSearchOverlay()
        });

        //mobile search trigger
        GlobalJQueryv1_11('.search-trigger a').on('click', function(e) {
            GlobalJQueryv1_11('#global-menu').removeClass('active');

            GlobalJQueryv1_11('.quicktools-title p').removeClass('full');

            GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');

            GlobalJQueryv1_11('.mobile-header.mobile-menu-trigger').removeClass('active');

            if (GlobalJQueryv1_11('.mobile-header .search-trigger').hasClass('active')) {

                GlobalJQueryv1_11('.mobile-header .search-trigger').removeClass('active');
                GlobalJQueryv1_11('#search--display').css('display', 'none');
                GlobalJQueryv1_11('#ge-overlay').removeClass('active');
                closeAll();
                unlockScroll();

            } else {
                positionSearchOverlay();

                GlobalJQueryv1_11('.mobile-header .search-trigger').addClass('active');
                GlobalJQueryv1_11('#search--display').css('display', 'block');
                GlobalJQueryv1_11('#ge-overlay').addClass('active');

                GlobalJQueryv1_11('#search--input').focus();

                lockScroll();
            }
        });

        // short cuts
        GlobalJQueryv1_11(".quick-tools--trigger").on('touchstart click', function(e) {
            GlobalJQueryv1_11("#shortcuts-menu").addClass("active");
            GlobalJQueryv1_11("#ge-overlay").addClass('active');

            if (locked) {
                unlockScroll();
            } else {
                lockScroll();
                GlobalJQueryv1_11('#search--display').removeClass("active");
                GlobalJQueryv1_11('#search--display').css("display", 'none');
            }
        });

        GlobalJQueryv1_11(".quicktools-container .shortcuts-close a").on('touchstart click', function(
            e) {
            GlobalJQueryv1_11("#shortcuts-menu").removeClass("active");
            GlobalJQueryv1_11("#ge-overlay").removeClass('active');

            if (locked) {
                unlockScroll();
            } else {
                lockScroll();
                GlobalJQueryv1_11('#search--display').removeClass("active");
                GlobalJQueryv1_11('#search--display').css("display", 'none');
            }
        });


        GlobalJQueryv1_11('#ge-overlay').on('touchstart click', function(e) {

            GlobalJQueryv1_11("#shortcuts-menu").removeClass('active');

            GlobalJQueryv1_11(".search--wrapper-hidden").removeClass('active');

            GlobalJQueryv1_11('#search--display').css('display', 'none');
            GlobalJQueryv1_11('#search--display').removeClass('active');
            GlobalJQueryv1_11('.menu--tier-one-category--search').removeClass('active');


            GlobalJQueryv1_11('.mobile-header.search-trigger').removeClass('active');

            GlobalJQueryv1_11("#ge-overlay").removeClass('active');
            //GlobalJQueryv1_11('#ge-overlay').addClass('non-active');
            if (locked) {
                unlockScroll();
            } else {
                lockScroll();
            }

            var mq = window.matchMedia("(max-width: 700px)");
            if (mq.matches) {
                GlobalJQueryv1_11('.mobile-header .search-trigger').removeClass('active');
                GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');

                GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');
                GlobalJQueryv1_11('.quicktools-open').removeClass('active');
                GlobalJQueryv1_11('.quicktools-open').css('height', '0px');
                GlobalJQueryv1_11('.quicktools-full').css('height', '0px');
                GlobalJQueryv1_11('.quicktools-title a p').text('Quick Tools').removeClass('full');
                GlobalJQueryv1_11('.qt-close').removeClass('active');
                GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');
                GlobalJQueryv1_11('.qt-boot').css('display', 'none').removeClass('active');
                GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');
                //GlobalJQueryv1_11('#ge-overlay').removeClass('active');
            }
        });


        //working
        //Mobile menu trigger displays dropdown mobile menu with commented out code for overlay
        GlobalJQueryv1_11('.mobile-header .mobile-menu-trigger').on('click', function(e) {

            GlobalJQueryv1_11('.quicktools-title p').removeClass('full');

            if (GlobalJQueryv1_11('#global-menu.menu').hasClass('active')) {
                closeAll();
                GlobalJQueryv1_11('.mobile-header .mobile-menu-trigger').removeClass('active');
                GlobalJQueryv1_11('.ge-overlay').addClass('active');

                //added code to reset open menu
                e.preventDefault();
                GlobalJQueryv1_11('.ge_parent').removeAttr('style').removeClass('ge_parent');

                unlockScroll();
            } else {

                GlobalJQueryv1_11('.menu--tier-one-category--search').removeClass('active');
                GlobalJQueryv1_11('.mobile-header .search-trigger').removeClass('active');
                GlobalJQueryv1_11('#search--display').removeClass("active");
                GlobalJQueryv1_11('#search--display').css("display", 'none');

                closeAll();
                GlobalJQueryv1_11('#global-menu.menu').addClass('active');
                GlobalJQueryv1_11('.mobile-header .mobile-menu-trigger').addClass('active');
                GlobalJQueryv1_11('.ge-overlay').addClass('active');

                lockScroll();
            }
        });

        //menu active in mobile

        GlobalJQueryv1_11('[placeholder]').focus(function() {
            var input = GlobalJQueryv1_11(this);
            if (input.val() == input.attr('placeholder')) {
                input.val('');
                //input.removeClass('placeholder');
            }
        }).blur(function() {
            var input = GlobalJQueryv1_11(this);
            if (input.val() == '' || input.val() == input.attr('placeholder')) {
                //input.addClass('placeholder');
                input.val(input.attr('placeholder'));
            }
        }).blur();
        GlobalJQueryv1_11('[placeholder]').parents('form').submit(function() {
            GlobalJQueryv1_11(this).find('[placeholder]').each(function() {
                var input = GlobalJQueryv1_11(this);
                if (input.val() == input.attr('placeholder')) {
                    input.val('');
                }
            })
        });


        if (!GlobalJQueryv1_11('.mobile-panels:hidden').length == 1) {

            // if menu item is clicked and it isn't chevron follow link
            GlobalJQueryv1_11('.menu .menu--tier-one li:has(ol)').not('.menu--tier-two li a').on('click',
                function(e) {
                    if (!GlobalJQueryv1_11(this).hasClass('ge_parent')) {
                        e.preventDefault();
                        tpOff = 0;
                        GlobalJQueryv1_11('.ge_parent').removeClass('ge_parent');
                        GlobalJQueryv1_11(this).addClass('ge_parent');
                        GlobalJQueryv1_11('#global-menu').animate({
                            scrollTop: tpOff
                        }, 10);
                        if (GlobalJQueryv1_11(this).offset().top - 50 < 0) {
                            tpOff = (GlobalJQueryv1_11(this).offset().top - 50) * -1;
                        } else {
                            tpOff = GlobalJQueryv1_11(this).offset().top - 50;
                        }
                        GlobalJQueryv1_11('#global-menu').animate({
                            scrollTop: tpOff
                        }, 300);
                    } else {
                        e.preventDefault();
                        GlobalJQueryv1_11(this).removeClass('ge_parent');
                        GlobalJQueryv1_11('#global-menu').animate({
                            scrollTop: 0
                        }, 10);

                    }
                });
            GlobalJQueryv1_11('.menu--tier-one-link span').not('.tier_one_chevron').on('click', function(
                e) {
                window.location = GlobalJQueryv1_11(this).parent().attr('href');
            });
            GlobalJQueryv1_11('.menu--tier-two li a').on('click', function(e) {
                window.location = GlobalJQueryv1_11(this).attr('href');
            });

            //homepage hero wrapper
            GlobalJQueryv1_11('.hero-wrapper').on('click', function(e) {
                window.location = GlobalJQueryv1_11('#buyNow').attr('href');
            });

            //click on overlay, remove parent class and close menu
            GlobalJQueryv1_11("#ge-overlay").on('touchstart click', function(e) {
                GlobalJQueryv1_11('.ge_parent').removeClass('ge_parent');

                GlobalJQueryv1_11('#global-menu.menu').removeClass('active');
                //GlobalJQueryv1_11('#global-menu.menu').addClass('non-active');
                GlobalJQueryv1_11('#ge-overlay').removeClass('mobile-active');

                GlobalJQueryv1_11('.menu--tier-one-category--search').toggleClass('active');
                //GlobalJQueryv1_11(".search--wrapper-hidden").removeClass('active');
            });
        }

        GlobalJQueryv1_11("#ge-overlay, .search--wrapper-hidden").bind(
            'touchmove',
            function(e) {
                e.preventDefault();
            }
        );


        // mobile quicktools

        GlobalJQueryv1_11(".quicktools-title, .qt-more").on('click', function(e) {
            e.preventDefault();
            var qt1 = GlobalJQueryv1_11('.quicktools-open').css('height');
            var qt2 = GlobalJQueryv1_11('.quicktools-full').css('height');

            if ((GlobalJQueryv1_11(window).width() < 373)) {

                // if qt1 and qt2 are closed (height = 0)
                if ((qt1 == '0px') && (qt2 == '0px')) {
                    closeAll();
                    GlobalJQueryv1_11('.quicktools-open').addClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-title a p').addClass('full').text('Close Tools');
                    GlobalJQueryv1_11('.mobile-quicktools').addClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').css('height', '94%');
                    GlobalJQueryv1_11('.qt-boot').css('display', 'block').addClass('active');
                    GlobalJQueryv1_11('.qt-close').addClass('active');

                    lockScroll();
                    GlobalJQueryv1_11('#ge-overlay').addClass('active');

                }

                // if qt1 and qt2 are not closed (height = 0)
                if ((qt1 != '0px') && (qt2 != '0px')) {
                    GlobalJQueryv1_11('.quicktools-open').removeClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '0');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '0');
                    GlobalJQueryv1_11('.quicktools-title a p').text('QuickTools').removeClass('full');
                    GlobalJQueryv1_11('.qt-close').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');
                    closeAll();
                    GlobalJQueryv1_11('.qt-boot').css('display', 'none').removeClass('active');

                    GlobalJQueryv1_11('.qt-close').removeClass('active');

                    unlockScroll();
                    GlobalJQueryv1_11('#ge-overlay').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');

                }
            }

            if ((GlobalJQueryv1_11(window).width() > 480) && ((GlobalJQueryv1_11(window).width() <= 736))) {
                // if qt1 and qt2 are closed (height = 0)
                if ((qt1 == '0px') && (qt2 == '0px')) {
                    closeAll();
                    GlobalJQueryv1_11('.quicktools-open').addClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-title a p').addClass('full').text('Close Tools');
                    GlobalJQueryv1_11('.mobile-quicktools').addClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').css('height', '94%');
                    GlobalJQueryv1_11('.qt-boot').css('display', 'block').addClass('active');
                    GlobalJQueryv1_11('.qt-close').addClass('active');

                    lockScroll();
                    GlobalJQueryv1_11('#ge-overlay').addClass('active');
                }

                // if qt1 and qt2 are not closed (height = 0)
                if ((qt1 != '0px') && (qt2 != '0px')) {
                    GlobalJQueryv1_11('.quicktools-open').removeClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '0');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '0');
                    GlobalJQueryv1_11('.quicktools-title a p').text('QuickTools').removeClass('full');
                    GlobalJQueryv1_11('.qt-close').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');
                    closeAll();
                    GlobalJQueryv1_11('.qt-boot').css('display', 'none').removeClass('active');

                    GlobalJQueryv1_11('.qt-close').removeClass('active');

                    unlockScroll();
                    GlobalJQueryv1_11('#ge-overlay').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');

                }
            }


            if ((GlobalJQueryv1_11(window).width() <= 538) && (GlobalJQueryv1_11(window).width() > 373)) {

                // if qt1 and qt2 are closed (height = 0)
                if ((qt1 == '0px') && (qt2 == '0px')) {
                    closeAll();
                    GlobalJQueryv1_11('.quicktools-open').addClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-title a p').addClass('partial').text('More Tools');
                    GlobalJQueryv1_11('.qt-close').addClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools').addClass('active');
                    GlobalJQueryv1_11('#ge-overlay').addClass('active');
                    GlobalJQueryv1_11('.qt-more').css('display', 'block').addClass('active');
                    lockScroll();
                } else if ((qt1 != '0px') && (qt2 == '0px')) {
                    // if qt1 and qt2 are closed (height = 0)
                    GlobalJQueryv1_11('.quicktools-full').css("height", "100%");
                    GlobalJQueryv1_11('.quicktools-title a p').addClass('full').text('Close Tools');
                    GlobalJQueryv1_11('.quicktools-full').addClass('active');
                    GlobalJQueryv1_11('.qt-more').css('display', 'none').removeClass('active');
                    GlobalJQueryv1_11('.qt-boot').css('display', 'block').addClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').css('height', '94%');
                } else if ((qt1 != '0px') && (qt2 != '0px')) {
                    // if qt1 and qt2 are open (height = 0)
                    GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');
                    GlobalJQueryv1_11('.quicktools-open').removeClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '0px');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '0px');
                    GlobalJQueryv1_11('.quicktools-title a p').text('Quick Tools').removeClass('full');
                    GlobalJQueryv1_11('.qt-close').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');
                    GlobalJQueryv1_11('.qt-boot').css('display', 'none').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');
                    GlobalJQueryv1_11('#ge-overlay').removeClass('active');

                    unlockScroll();
                }
            }

            if ((GlobalJQueryv1_11(window).width() > 538) && (GlobalJQueryv1_11(window).width() < 959)) {
                // if qt1 and qt2 are closed (height = 0)
                if ((qt1 == '0px') && (qt2 == '0px')) {
                    closeAll();
                    GlobalJQueryv1_11('.quicktools-open').addClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '100%');
                    GlobalJQueryv1_11('.quicktools-title a p').addClass('full').text('Close Tools');
                    GlobalJQueryv1_11('.mobile-quicktools').addClass('active');
                    lockScroll();
                    GlobalJQueryv1_11('#ge-overlay').addClass('active');
                    GlobalJQueryv1_11('.qt-boot').css('display', 'block').addClass('active');

                }

                // if qt1 and qt2 are not closed (height = 0)
                if ((qt1 != '0px') && (qt2 != '0px')) {
                    GlobalJQueryv1_11('.quicktools-open').removeClass('active');
                    GlobalJQueryv1_11('.quicktools-open').css('height', '0');
                    GlobalJQueryv1_11('.quicktools-full').css('height', '0');
                    GlobalJQueryv1_11('.quicktools-title a p').text('QuickTools').removeClass('full');
                    GlobalJQueryv1_11('.qt-close').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');
                    GlobalJQueryv1_11('.qt-boot').css('display', 'none').removeClass('active');
                    closeAll();
                    unlockScroll();
                    GlobalJQueryv1_11('#ge-overlay').removeClass('active');
                    GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');

                }

            }

        });

        GlobalJQueryv1_11('.qt-close, .qt-boot').on('click', function(e) {
            e.preventDefault();
            GlobalJQueryv1_11('.quicktools-open').removeClass('active');
            GlobalJQueryv1_11('.quicktools-open').css('height', '0px');
            GlobalJQueryv1_11('.quicktools-full').css('height', '0px');
            GlobalJQueryv1_11('.quicktools-title a p').text('QuickTools').removeClass('full');
            GlobalJQueryv1_11('.qt-close').removeClass('active');
            GlobalJQueryv1_11('.mobile-quicktools.active').removeAttr('style');
            GlobalJQueryv1_11('.mobile-quicktools').removeClass('active');

            GlobalJQueryv1_11('.qt-boot').css('display', 'none').removeClass('active');

            unlockScroll();
            GlobalJQueryv1_11('#ge-overlay').removeClass('active');

        });
        // end mobile quicktools


        //place language selector
        //assign bg color to language
        if (GlobalJQueryv1_11("html").hasClass("no-touch")) {
            GlobalJQueryv1_11('.utility-links #nav-tool-multilingual-header').hover(function(e) {
                placeLanguage();
                GlobalJQueryv1_11('.utility-links #lang_select').addClass('active');
                GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').addClass('highlight');
            }, function(e) {
                GlobalJQueryv1_11('.utility-links #lang_select').removeClass('active');
                GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').removeClass('highlight');
            });
            GlobalJQueryv1_11('#lang_select .content div').hover(function(e) {
                GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').addClass('highlight');
            }, function(e) {
                GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').removeClass('highlight');
            });
        }

        if (GlobalJQueryv1_11("html").hasClass("touch")) {
            GlobalJQueryv1_11('.utility-links #nav-tool-multilingual-header').on('touchstart click', function(e) {
                    placeLanguage();
                    GlobalJQueryv1_11('.utility-links #lang_select').toggleClass('active');
                    GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').toggleClass('highlight');
            });
            GlobalJQueryv1_11('#lang_select .content div').on('click', function(e) {
                GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').toggleClass('highlight');
                GlobalJQueryv1_11('.nav-window-header').toggleClass('active');

            });
			GlobalJQueryv1_11('#lang_select .content div').hover(function(e){
				GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').addClass('highlight');
			}, function(e){
				GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').removeClass('highlight');
			});

            GlobalJQueryv1_11('.nav-tool-header .anchor').on('click', function(e) {
                GlobalJQueryv1_11('.nav-window-header').toggleClass('active');
                GlobalJQueryv1_11('.utility-links .nav-tool-header a.anchor').toggleClass('highlight');
            });
        }


        function placeLanguage() {
            var lft = GlobalJQueryv1_11('.utility-links #nav-tool-multilingual-header').offset().left;
            var tp = GlobalJQueryv1_11('.utility-links #nav-tool-multilingual-header').offset().top;
            GlobalJQueryv1_11("div#lang_select").offset({
                top: tp + 20,
                left: lft
            });
        }

</script>


